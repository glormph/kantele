# PostgreSQL
PG_SUPERUSER_PASSWORD={{ pg_superuser_pass }}
PG_KANTELE_PASSWORD={{ db_pass }}
KANTELE_DB_NAME={{ kanteledb }}

# message queue
RABBIT_VHOST={{ mq_vhost }}
RABBITUSER={{ rabbituser }}
RABBITPASS={{ amqppass }}

# data, uploads, servable content, qc.html etc
PG_DATA_PATH={{ kanteledir }}/pgdata
HOST_UPLOADPATH={{ kanteledir }}/uploads
HOST_ANALYSISFILES={{ kanteledir }}/srv_analysis

# /path/to/your/backups
BACKUP_PATH=""

# mydomain.example.com for Django allowed-hosts setting
HOST_FQDN={{ kantele_host }}

# Inside containers
CONTAINER_UPLOADPATH=/uploads
CONTAINER_ANALYSISPATH=/analysisfiles

# Logging, TDB
SYSLOG_ADDRESS=unix:///dev/log
